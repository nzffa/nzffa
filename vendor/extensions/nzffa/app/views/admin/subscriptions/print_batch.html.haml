%link{:rel => 'stylesheet', :type => 'text/css', :href => '/stylesheets/normalize.css'}
%link{:rel => 'stylesheet', :type => 'text/css', :href => '/css/subscription-form.css'}
= render :partial => 'subscriptions/print_css'

-@unsaved_orders.each do |order|
  -@order = order
  -@subscription = order.subscription
  -@reader = @subscription.reader
  =render :partial => 'subscriptions/print'
  .page-break
